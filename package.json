{
  "name": "tabcorp-cucumber-protractor-framework-v2",
  "version": "1.0.47",
  "description": "common cucumber steps written with protractor",
  "main": "dist/index.js",
  "typings": "dist/index",
  "engines": {
    "node": ">=6.9.2"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "build": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "test": "ts-mocha -p ./tsconfig.json ./src/unit/**/*.spec.ts --reporter mochawesome",
    "e2e": "shx rm -rf dist && tsc && ./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/protractor ./dist/e2e/config/protractor.conf.js",
    "clean": "shx rm -rf dist"
  },
  "keywords": [
    "tabcorp",
    "cucumber protractor protractor",
    "cucumber steps",
    "cucumber step definitions",
    "protractor",
    "test",
    "testing",
    "automated tests",
    "e2e tests",
    "end-to-end tests",
    "webdriver",
    "webdriverjs",
    "selenium",
    "cucumber",
    "cucumberjs",
    "cucumber-js",
    "gherkin",
    "bdd"
  ],
  "author": "tabcorp",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/chai-spies": "^1.0.0",
    "@types/cucumber": "^4.0.4",
    "@types/expect": "^1.20.3",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "@types/promise.prototype.finally": "^2.0.1",
    "@types/puppeteer": "^1.9.0",
    "@types/selenium-webdriver": "^3.0.10",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chai-spies": "^1.0.0",
    "cucumber": "^4.2.1",
    "cucumber-html-reporter": "^4.0.2",
    "cucumber-pretty": "^1.4.0",
    "del": "^3.0.0",
    "gherkin-lint": "^3.3.6",
    "inversify": "^5.0.1",
    "js-yaml": "^3.12.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "mocha-teamcity-reporter": "^2.5.0",
    "mochawesome": "^3.0.3",
    "multiple-cucumber-html-reporter": "^1.10.2",
    "protractor": "^7.0.0",
    "protractor-cucumber-framework": "^4.2.0",
    "protractor-multiple-cucumber-html-reporter-plugin": "^1.7.1",
    "puppeteer": "^1.9.0",
    "reflect-metadata": "^0.1.12",
    "request-promise": "^4.2.5",
    "shelljs": "^0.8.3",
    "shx": "^0.3.2",
    "ts-mocha": "^2.0.0",
    "ts-node": "^7.0.1",
    "typescript": "3.6.4",
    "webdriver-manager": "^12.1.0"
  },
  "dependencies": {
    "selenium-webdriver": "^4.0.0-alpha.1",
    "moment-timezone": "^0.5.25",
    "faker": "^4.1.0"
  }
}
